{
    "name": "oskonnikov/email-checker-php",
    "description": "PHP Package To Validate If An Email Address Exists Without Sending An Email",
    "keywords": [
        "php",
        "email",
        "validate",
        "SMTP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg S.",
            "email": "devopwr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0|^8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Oskonnikov\\EmailCheckerPHP\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oskonnikov\\EmailCheckerPHP\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
